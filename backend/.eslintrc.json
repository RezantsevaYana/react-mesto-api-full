{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "quotes": ["error", "double"],
      "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
